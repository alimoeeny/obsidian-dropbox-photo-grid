{
    "name": "obsidian-dropbox-photo-grid",
    "version": "1.0.0",
    "description": "An Obsidian plugin for embedding Dropbox photos in a grid layout",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json"
    },
    "keywords": ["obsidian", "plugin", "dropbox", "photos"],
    "author": "Your Name",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^16.11.6",
        "obsidian": "^1.4.0",
        "typescript": "4.7.4",
        "esbuild": "0.17.3",
        "builtin-modules": "^3.3.0"
    },
    "dependencies": {
        "dropbox": "^10.34.0",
        "tslib": "^2.6.2"
    }
}
